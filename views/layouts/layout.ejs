    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!--STYLES-->
    <link rel="stylesheet" href="/node/CompensacionElecciones/min/production.min.css">
    <!--STYLES END-->
  </head>

  <body class="p-3">
